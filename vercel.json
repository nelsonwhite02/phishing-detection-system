{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "build": {
    "env": {
      "CARGO_HOME": "/tmp/.cargo",
      "RUSTUP_HOME": "/tmp/.rustup"
    },
    "scripts": {
      "build": "curl https://sh.rustup.rs -sSf | sh -s -- -y && rustc --version && pip install -r requirements.txt"
    }
  }
}
